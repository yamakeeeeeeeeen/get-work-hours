{
  "name": "get-work-time",
  "version": "1.0.0",
  "license": "MIT",
  "main": "main/index.js",
  "productName": "GetWorkTime",
  "scripts": {
    "build": "yarn build-renderer && yarn build-electron",
    "build-electron": "tsc -p electron-src",
    "build-renderer": "next build renderer && next export renderer",
    "clean": "rimraf dist main renderer/out renderer/.next",
    "dev": "yarn build-electron && electron .",
    "dist": "yarn build && electron-builder",
    "pack-app": "yarn build && electron-builder --dir",
    "type-check": "tsc --project renderer/tsconfig.json",
    "eslint": "eslint ./renderer --ext .tsx --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "electron-is-dev": "^1.1.0",
    "electron-next": "^3.1.5",
    "luxon": "^1.25.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hook-form": "^6.11.2"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.4.2",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.15",
    "@types/luxon": "^1.25.0",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.9",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-jest": "^26.6.3",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "next": "latest",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "renderer/out"
    ]
  }
}
